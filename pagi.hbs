<nav aria-label="Page navigation example">
					<ul class="pagination">


						{{#if (equal linkpagecurrent 1)}}
						<li class="page-item disabled"><a class="page-link" href={{previousPage}}>Previous</a></li>
						{{else}}
						<li class="page-item"><a class="page-link" href={{previousPage}}>Previous</a></li>
						{{/if}}


						<li class="page-item active"><a class="page-link" href={{linkpagecurrent}}>{{pagecurrent}}</a>
						</li>


						{{#forabc 0 3 1 pagecurrent totalpages}}

						<li class="page-item"><a class="page-link" href={{linkhasnextpage}}>{{numnextpage}}</a></li>

						{{/forabc}}



						{{#if (equal linkpagecurrent totalpages)}}
						<li class="page-item disabled"><a class="page-link" href={{nextpage}}>Next</a></li>
						{{else}}
						<li class="page-item"><a class="page-link" href={{nextpage}}>Next</a></li>
						{{/if}}
					</ul>
				</nav>